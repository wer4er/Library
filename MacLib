    Notifications.Name = "Notifications"
    Notifications.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Notifications.BackgroundTransparency = 1
    Notifications.BorderColor3 = Color3.fromRGB(0, 0, 0)
